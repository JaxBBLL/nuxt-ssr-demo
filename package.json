{
  "name": "nuxt-ssr-demo",
  "version": "1.0.0",
  "description": "My fabulous Nuxt.js project",
  "author": "JaxBBLL",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "qa": "cross-env MY_ENV=qa nuxt build",
    "uat": "cross-env MY_ENV=uat nuxt build",
    "prod": "cross-env MY_ENV=prod nuxt build",
    "start": "nuxt start",
    "pm2": "pm2 start --name nuxtDemo ./node_modules/nuxt/bin/nuxt.js -- start",
    "serve:qa": "npm run qa && npm run pm2",
    "serve:uat": "npm run uat && npm run pm2",
    "serve:prod": "npm run prod && npm run pm2",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .gitignore ."
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.12.5",
    "element-ui": "^2.15.0",
    "nuxt": "^2.14.12"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^5.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/proxy": "^2.1.0",
    "@nuxtjs/tailwindcss": "^3.4.2",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.18.0",
    "eslint-plugin-nuxt": ">=2.0.0",
    "less": "^4.1.0",
    "less-loader": "^7.3.0"
  }
}
